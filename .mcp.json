{
  "mcpServers": {
    "tauri": {
      "command": "npx",
      "args": ["-y", "@hypothesi/tauri-mcp-server"],
      "env": {
        "TAURI_MCP_URL": "ws://localhost:9324"
      }
    },
    "codex": {
      "command": "codex",
      "args": ["mcp-server"]
    },
    "chrome-devtools": {
      "command": "npx",
      "args": ["-y", "chrome-devtools-mcp@latest"]
    }
  }
}
